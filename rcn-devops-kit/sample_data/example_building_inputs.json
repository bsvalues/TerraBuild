  "calculation_examples": [
    {
      "example_id": "RES-SFR-01",
      "calculation_steps": [
        {
          "step": 1,
          "description": "Determine base rate",
          "details": "Single-family residential base rate = $125.00/sq ft"
        },
        {
          "step": 2,
          "description": "Apply construction type multiplier",
          "details": "Frame construction multiplier = 1.0 (no adjustment)"
        },
        {
          "step": 3,
          "description": "Apply quality class multiplier",
          "details": "Standard quality multiplier = 1.0 (no adjustment)"
        },
        {
          "step": 4,
          "description": "Apply regional cost multiplier",
          "details": "Midwest region multiplier = 1.0 (no adjustment)"
        },
        {
          "step": 5,
          "description": "Calculate adjusted base rate",
          "details": "$125.00 × 1.0 × 1.0 × 1.0 = $125.00/sq ft"
        },
        {
          "step": 6,
          "description": "Calculate base building cost",
          "details": "$125.00 × 2,200 sq ft = $275,000"
        },
        {
          "step": 7,
          "description": "Calculate basement cost",
          "details": "$125.00 × 0.4 × 1,100 sq ft = $55,000"
        },
        {
          "step": 8,
          "description": "Calculate garage cost",
          "details": "$45.00 × 440 sq ft = $19,800"
        },
        {
          "step": 9,
          "description": "Calculate feature costs",
          "details": "Fireplace: $7,500 × 1 = $7,500\nHVAC: $10.00 × 2,200 sq ft = $22,000"
        },
        {
          "step": 10,
          "description": "Calculate total replacement cost new",
          "details": "$275,000 + $55,000 + $19,800 + $7,500 + $22,000 = $379,300"
        },
        {
          "step": 11,
          "description": "Calculate age-based depreciation",
          "details": "15 years old, residential depreciation rate = 0.15 (15%)"
        },
        {
          "step": 12,
          "description": "Calculate condition-based adjustment",
          "details": "Average condition, adjustment = 0.25 (25%)"
        },
        {
          "step": 13,
          "description": "Calculate total depreciation",
          "details": "Max of 0.15 and 0.25 = 0.25 (25%)"
        },
        {
          "step": 14,
          "description": "Calculate depreciated cost",
          "details": "$379,300 × (1 - 0.25) = $284,475"
        }
      ],
      "result": {
        "replacement_cost_new": 379300,
        "depreciation_percentage": 25,
        "depreciated_value": 284475
      }
    }
  ]
