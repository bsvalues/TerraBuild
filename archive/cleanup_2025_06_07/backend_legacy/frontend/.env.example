# TerraBuild Frontend Environment Variables
# Copy this file to .env.local for local development

# API URL - Set this to your backend API URL
REACT_APP_API_URL=http://localhost:5001

# Jurisdiction Configuration - Set these for your county
REACT_APP_JURISDICTION=Benton County, WA
REACT_APP_REGION=Eastern Washington

# Optional Settings
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_ENABLE_ANALYTICS=false